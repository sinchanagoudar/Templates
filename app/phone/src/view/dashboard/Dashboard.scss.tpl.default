$dashboard-item-spacing: dynamic(20px);

.dashboard > .x-body-el {
    padding: $dashboard-item-spacing 0 0 $dashboard-item-spacing;
}

.dashboard-item {
    margin: 0 $dashboard-item-spacing $dashboard-item-spacing 0;
    float: left;
    width: calc(100% - #\u007B$dashboard-item-spacing});

    &.last-in-row {
        margin-right: 0;
    }
}

.small-100 {
    width: calc(100% - #\u007B$dashboard-item-spacing});
}

.small-60 {
    width: calc(60% - #\u007B$dashboard-item-spacing});
}

.small-50 {
    width: calc(50% - #\u007B$dashboard-item-spacing});
}

.small-40 {
    width: calc(40% - #\u007B$dashboard-item-spacing});
}

.small-20 {
    width: calc(20% - #\u007B$dashboard-item-spacing});
}

@media (min-width: 1000px) {
    .big-100 {
        width: calc(100% - #\u007B$dashboard-item-spacing});
    }

    .big-60 {
        width: calc(60% - #\u007B$dashboard-item-spacing});
    }

    .big-50 {
        width: calc(50% - #\u007B$dashboard-item-spacing});
    }

    .big-40 {
        width: calc(40% - #\u007B$dashboard-item-spacing});
    }

    .big-33 {
        width: calc(33.3% - #\u007B$dashboard-item-spacing});
    }

    .big-20 {
        width: calc(20% - #\u007B$dashboard-item-spacing});
    }
}

// Custom Tool type: 'wrench'
.#\u007B$prefix}tool-type-wrench {
    @include font-icon($fa-var-wrench);
    background: none;
}

.quick-graph-panel {
    .x-panel-header {
        padding-left: 1.375em;
    }

    .x-panel-title-icon {
        position: relative;

        &:before {
            position: absolute;
            height: 1.875em;
            width: 1.875em;
            top: -.4375em;
            left: -.4375em;
            border-radius: 1.875em;
            background-color: #f1f1f1;
            line-height: 1.875em;
            text-align: center;
        }
    }

    .x-panel-title-text {
        padding-left: .53em;
    }
}
